{
  "name": "jax-js-mcmc",
  "version": "0.1.0",
  "description": "HMC sampling library for jax-js",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./diagnostics": {
      "import": "./dist/diagnostics.js",
      "types": "./dist/diagnostics.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:physics": "vitest run tests/physics",
    "test:posteriors": "vitest run tests/posteriors",
    "test:reference": "vitest run tests/reference"
  },
  "peerDependencies": {
    "@jax-js/jax": ">=0.1.0"
  },
  "devDependencies": {
    "@jax-js/jax": "^0.1.5",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "mcmc",
    "hmc",
    "jax",
    "sampling",
    "bayesian"
  ],
  "license": "MIT"
}
